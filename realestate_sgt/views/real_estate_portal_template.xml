    <template id="portal_contract_page" name="Contract Portal Template">
        <t t-call="portal.portal_layout">
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                    <div class="row no-gutters">
                        <div class="col-12">
                            <h5 class="d-flex mb-1 mb-md-0 row">
                                <div class="col-9">
                                    <span t-field="contract_obj.name" class="text-truncate"/>
                                    <small
                                            class="text-muted d-none d-md-inline"
                                    >(
                                        <span
                                                t-field="contract_obj.name"
                                        />
                                        )
                                    </small>
                                </div>
                            </h5>
                        </div>
                    </div>
                </t>

                <t t-set="card_body">
                    <div class="row mb-4">
                        <div class="col-12 col-md-6" name="portal_ticket_col_0">
                            <div>
                                <strong>Contract Name:</strong>
                                <span t-field="contract_obj.contract_id.name"/>
                            </div>
                            <div>
                                <strong>Contract Number:</strong>
                                <span t-field="contract_obj.name"/>
                            </div>
                            <div>
                                <strong>Property:</strong>
                                <span t-field="contract_obj.property_id.name"/>
                            </div>
                            <div>
                                <strong>Unit:</strong>
                                <span t-field="contract_obj.unit_id.name"/>
                            </div>
                            <div>
                                <strong>Expired Date:</strong>
                                <span t-field="contract_obj.to_date"/>
                            </div>
                            <div>
                                <strong>Total Rent:</strong>
                                <span t-field="contract_obj.deposite"/>
                            </div>
                            <div>
                                <strong>Rent Offer :</strong>
                                <span t-field="contract_obj.offer_price"/>
                            </div>
                            <div>
                                <strong>Property Renter:</strong>
                                <span t-field="contract_obj.partner_id.name"/>
                            </div>
                            <div>
                                <strong>Renewal Date:</strong>
                                <span t-field="contract_obj.renewal_date" t-options='{"widget": "date"}'/>
                            </div>
                            <div>
                                <strong>Owner:</strong>
                                <span t-field="contract_obj.owner_id"/>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </t>
    </template>


