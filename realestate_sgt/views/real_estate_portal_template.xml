<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template
            id="portal_layout_maitenance"
            name="Portal layout: tttt menu entry"
            inherit_id="portal.portal_breadcrumbs"
            priority="40"
    >
        <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
            <li
                    t-if="page_name == 'ticket' or ticket"
                    t-attf-class="breadcrumb-item #{'active ' if not ticket else ''}"
            >

                <a t-if="ticket"
                   t-attf-href="/my/tickets1?{{ keep_query() }}"
                >Tickets
                </a>
                <t t-else="">Maintenance</t>
            </li>
            <li t-if="ticket" class="breadcrumb-item active text-truncate">
                <span t-field="ticket.name"/>
            </li>
        </xpath>
    </template>


    <template id="portal_my_home_contract_details" name="Show contract in portal" customize_show="True"
              inherit_id="portal.portal_my_home" priority="20">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Contract Details</t>
                <t t-set="url" t-value="'/my/contract'"/>
                <t t-set="placeholder_count" t-value="'contract_count'"/>

            </t>
        </xpath>
    </template>


    <template id="portal_my_contracts" name="My Contracts">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>

            <t t-call="portal.portal_searchbar">
                <t t-set="title">contracts</t>
            </t>
            <t t-if="contracts" t-call="portal.portal_table">
                <thead>
                    <tr class="active">
                        <th>Contract Name</th>
                        <th>Contract Number</th>
                        <th>Property</th>
                        <th>Unit</th>
                        <th>Start Date</th>
                        <th>Expired Date</th>
                        <th>Total Rent</th>
                        <th>Rent Offer</th>
                        <th>Property Renter</th>
                        <th>Renewal Date</th>
                        <th>Owner</th>
                        <th>Contract Month</th>
                    </tr>
                </thead>
                <t t-foreach="contracts" t-as="contract">
                    <tr>
                        <td>
                            <t t-out="contract.contract_id.name"/>
                        </td>
                        <td>
                            <a
                                    t-attf-href="/my/contract/#{contract.id}?{{ keep_query() }}"
                            >
                                <span t-field="contract.name"/>
                            </a>
                        </td>
                        <td>
                            <span t-field="contract.property_id.name"/>
                        </td>
                        <td>
                            <span t-field="contract.unit_id.name"/>
                        </td>
                        <td>
                            <span t-field="contract.from_date"/>
                        </td>
                        <td>
                            <span t-field="contract.to_date"/>
                        </td>
                        <td>
                            <span t-field="contract.deposite"/>
                        </td>
                        <td>
                            <span t-field="contract.discount_offer"/>
                        </td>
                        <td>
                            <span t-field="contract.partner_id.name"/>
                        </td>
                        <td>
                            <span t-field="contract.owner_id.name"/>
                        </td>
                        <td>
                            <span t-field="contract.contract_month"/>
                        </td>
                    </tr>
                </t>
            </t>
            <p t-else="">There are currently no contracts for your account.</p>
        </t>
    </template>

    <template id="portal_my_home_property_maintenance" name="Show Maintenance in portal" customize_show="True"
              inherit_id="portal.portal_my_home" priority="20">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Property Mailntenance</t>
                <t t-set="url" t-value="'/my/maintenance'"/>
                <t t-set="placeholder_count" t-value="'maintenance_count'"/>

            </t>
        </xpath>
    </template>


    <template id="portal_my_maintenance" name="My Property maintenance">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>

            <t t-call="portal.portal_searchbar">
                <t t-set="title">Maintenance</t>
            </t>
            <t t-if="maintenances" t-call="portal.portal_table">
                <thead>
                    <tr class="active">
                        <th>Maintenance</th>
                        <th>Date</th>
                        <th>Property</th>
                        <th>Cost</th>
                        <th>Operation</th>
                        <th>Status</th>
                    </tr>
                </thead>
                <t t-foreach="maintenances" t-as="maintenance">
                    <tr>
                        <td>
                            <a
                                    t-attf-href="/my/ticket1/#{maintenance.id}?{{ keep_query() }}"
                            >
                                <span t-field="maintenance.name"/>
                            </a>
                        </td>
                        <td>
                            <span t-field="maintenance.date"/>
                        </td>
                        <td>
                            <span t-field="maintenance.property_id.name"/>
                        </td>
                        <td>
                            <span t-field="maintenance.maintain_cost"/>
                        </td>
                        <td>
                            <span t-field="maintenance.operation"/>
                        </td>
                        <td>
                            <span t-field="maintenance.state"/>
                        </td>
                    </tr>
                </t>
            </t>
            <p t-else="">There are currently no repairs for your account.</p>
        </t>
    </template>


    <template
            id="portal_my_home_maintenance"
            name="Portal My Home : Maintenance entries"
            inherit_id="portal.portal_my_home"
            priority="40"
            customize_show="True"
    >
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Maintenances</t>
                <t t-set="url" t-value="'/my/tickets1'"/>
                <t t-set="placeholder_count" t-value="'maintenance_count'"/>
            </t>
            <form method="POST" t-attf-action="/new/maintenance">
                <input
                        type="hidden"
                        name="csrf_token"
                        t-att-value="request.csrf_token()"
                />
                <button
                        name="create_new_maintenance"
                        type="action"
                        class="btn btn-primary"
                        style="float: right; margin-right: 0px; margin-bottom: 5px;"
                >New Maintenance
                </button>
            </form>
        </xpath>
    </template>

    <template id="portal_my_tickets1" name="My maintenances">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True"/>

            <t t-call="portal.portal_searchbar">
                <t t-set="title">Maintenance</t>
            </t>

            <form method="POST" t-attf-action="/new/maintenance">
                <button
                        name="create_new_ticket"
                        type="action"
                        class="btn btn-primary"
                        style="float: right; margin-right: 0px; margin-bottom: 5px;"
                >New Maintenance
                </button>
                <input
                        type="hidden"
                        name="csrf_token"
                        t-att-value="request.csrf_token()"
                />
            </form>

            <t t-if="not grouped_tickets">
                <div class="alert alert-warning mt8" role="alert">
                    <p>No maintenance found.</p>
                </div>
            </t>
            <t t-call="realestate_sgt.portal_ticket_list1"/>
        </t>
    </template>

    <template id="portal_ticket_list1" name="Ticket List">
        <t t-if="grouped_tickets">
            <t t-call="portal.portal_table">
                <t t-foreach="grouped_tickets" t-as="tickets">
                    <thead>
                        <tr
                                t-attf-class="{{'thead-light' if not groupby == 'none' else ''}}"
                        >
                            <th class="text-left">Maintenance</th>
                            <th class="text-left">Property</th>
                            <th>Operation</th>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-foreach="tickets" t-as="ticket">
                            <tr>
                                <td>
                                    <a
                                            t-attf-href="/my/ticket/#{ticket.id}?{{ keep_query() }}"
                                    >
                                        <span t-field="ticket.name"/>
                                    </a>
                                </td>
                                <td class="text-left">
                                    <span t-esc="ticket.property_id.name"/>
                                </td>
                                <td>
                                    <span t-field="ticket.operation"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </t>
            </t>
        </t>
    </template>


    <template id="portal_helpdesk_ticket_page1" name="Ticket Portal Template1">
        <t t-call="portal.portal_layout">
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                    <a t-if="maintenance_ids"
                       t-attf-href="/my/tickets1?{{ keep_query() }}"
                    >Maintenances
                    </a>
                    <div class="row no-gutters">
                        <div class="col-12">
                            <h5 class="d-flex mb-1 mb-md-0 row">
                                <div class="col-9">
                                    <!--                                     t-if="ticket"-->
                                    <span t-field="maintenance_ids.name" class="text-truncate"/>
                                    <small
                                            class="text-muted d-none d-md-inline"
                                    >(
                                        <span
                                                t-field="maintenance_ids.name"
                                        />
                                        )
                                    </small>
                                </div>
                            </h5>
                        </div>
                    </div>
                </t>

                <t t-set="card_body">
                    <div class="row mb-4">
                        <div class="col-12 col-md-6" name="portal_ticket_col_0">
                            <div>
                                <strong>Maintenance:</strong>
                                <!--                            <t t-esc='maintenance_ids.name'/>-->
                                <span t-field="maintenance_ids.name"/>
                            </div>
                            <div>
                                <strong>Property:</strong>
                                <span t-field="maintenance_ids.property_id"/>
                            </div>
                            <div>
                                <strong>Responsible:</strong>
                                <span t-field="maintenance_ids.responsible_id"/>
                            </div>
                            <div>
                                <strong>Unit:</strong>
                                <span t-field="maintenance_ids.unit_id"/>
                            </div>
                        </div>
                        <div
                                class="col-12 col-md-6 text-right"
                                name="portal_ticket_col_1"
                        >
                            <div>
                                <strong>Date:</strong>
                                <span
                                        t-field="maintenance_ids.date"
                                        t-options='{"widget": "date"}'
                                />
                            </div>
                            <div>
                                <strong>Operation:</strong>
                                <span t-field="maintenance_ids.operation"/>
                            </div>
                            <div>
                                <strong>Status:</strong>
                                <span t-field="maintenance_ids.state"/>
                            </div>
                        </div>
                    </div>

                    <div class="row" t-if="maintenance_ids.description or maintenance_ids.attachment_ids">
                        <div
                                t-if="maintenance_ids.description"
                                t-attf-class="col-12 col-lg-7 mb-4 mb-md-0 {{'col-lg-7' if maintenance_ids.attachment_ids else 'col-lg-12'}}"
                        >
                            <hr
                                    class="mb-1"
                            />
                            <div
                                    class="d-flex my-2"
                            >
                                <strong
                                >Description
                                </strong>
                            </div>
                            <div
                                    class="py-1 px-2 bg-100 small"
                                    t-raw="maintenance_ids.description"/>
                        </div>
                        <div
                                t-if="maintenance_ids.attachment_ids"
                                t-attf-class="col-12 col-lg-5 o_project_portal_attachments {{'col-lg-5' if maintenance_ids.description else 'col-lg-12'}}"
                        >
                            <hr
                                    class="mb-1 d-none d-lg-block"
                            />
                            <strong
                                    class="d-block mb-2"
                            >Attachments
                            </strong>
                            <div
                                    class="row"
                            >
                                <div
                                        t-attf-class="col {{'col-lg-6' if not maintenance_ids.description else 'col-lg-12'}}"
                                >
                                    <ul
                                            class="list-group"
                                    >
                                        <a
                                                class="list-group-item list-group-item-action d-flex align-items-center oe_attachments py-1 px-2"
                                                t-foreach='maintenance_ids.attachment_ids'
                                                t-as='attachment'
                                                t-attf-href="/web/content/#{attachment.id}?download=true&amp;access_token=#{attachment.access_token}"
                                                target="_blank"
                                                data-no-post-process=""
                                        >
                                            <div
                                                    class='oe_attachment_embedded o_image o_image_small mr-2 mr-lg-3'
                                                    t-att-title="attachment.name"
                                                    t-att-data-mimetype="attachment.mimetype"
                                                    t-attf-data-src="/web/image/#{attachment.id}/50x40?access_token=#{attachment.access_token}"
                                            />
                                            <div
                                                    class='oe_attachment_name text-truncate'
                                            >
                                                <t t-esc='attachment.name'/>
                                            </div>
                                        </a>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </t>
            </t>

            <div class="mt32">
                <h4>
                    <strong
                    >Message and communication history
                    </strong>
                </h4>
                <!--                                <t-->
                <!--                                        t-call="portal.message_thread"-->
                <!--                                >-->
                <!--                                    <t-->
                <!--                                            t-set="object"-->
                <!--                                            t-value="maintenance_ids"-->
                <!--                                    />-->
                <!--                                    <t-->
